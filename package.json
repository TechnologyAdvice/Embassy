{
  "name": "embassy",
  "version": "0.3.1",
  "description": "JWT token generator/verifier with built-in permission flags",
  "main": "src/Embassy.js",
  "engines": {
    "node": ">=4.2"
  },
  "scripts": {
    "clean": "rm -rf node_modules coverage",
    "test": "npm run lint && npm run test-cov && npm run check-cov",
    "mocha": "mocha",
    "test-cov": "istanbul cover _mocha",
    "check-cov": "istanbul check-coverage --statements 95 --branches 95 --functions 100 --lines 100",
    "lint": "eslint ./src ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TomFrost/embassy.git"
  },
  "keywords": [
    "JWT",
    "token",
    "oauth",
    "oauth2",
    "openid",
    "connect",
    "permission",
    "permissions",
    "flags"
  ],
  "author": "Tom Shawver <tom@frosteddesign.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TomFrost/embassy/issues"
  },
  "homepage": "https://github.com/TomFrost/embassy#readme",
  "dependencies": {
    "base64-js": "^1.3.0",
    "jsonwebtoken": "^8.3.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "delay": "^3.1.0",
    "eslint": "^5.4.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0"
  }
}
